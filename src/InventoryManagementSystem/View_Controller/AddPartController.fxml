<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="450.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Add Part">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </Label>
      <RadioButton fx:id="outsourcedBtn" layoutX="310.0" layoutY="18.0" mnemonicParsing="false" onAction="#onActionOutsourcedBtn" text="Outsourced">
         <font>
            <Font size="14.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="inHouseBtn" layoutX="160.0" layoutY="18.0" mnemonicParsing="false" onAction="#onActionInHouseBtn" selected="true" text="In-House">
         <font>
            <Font size="14.0" />
         </font>
      </RadioButton>
      <Label layoutX="50.0" layoutY="60.0" text="ID">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField disable="true" editable="false" layoutX="160.0" layoutY="60.0" text="Auto Gen - Disabled">
         <font>
            <Font name="System Italic" size="12.0" />
         </font></TextField>
      <Label layoutX="50.0" layoutY="95.0" text="Name">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="nameInput" layoutX="160.0" layoutY="95.0" />
      <Label layoutX="50.0" layoutY="130.0" text="Inv">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="invInput" layoutX="160.0" layoutY="130.0" />
      <Label layoutX="50.0" layoutY="165.0" text="Price/Cost">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="priceInput" layoutX="160.0" layoutY="165.0" />
      <Label layoutX="50.0" layoutY="200.0" text="Max">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="maxInput" layoutX="160.0" layoutY="200.0" />
      <Label layoutX="335.0" layoutY="200.0" text="Min">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="minInput" layoutX="380.0" layoutY="200.0" />
      <Label fx:id="machineCompanyIDLabel" layoutX="50.0" layoutY="235.0" text="Machine ID">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="machineCompanyIdInput" layoutX="160.0" layoutY="235.0" />
      <Button fx:id="saveBtn" layoutX="455.0" layoutY="289.0" mnemonicParsing="false" onAction="#onActionSaveBtn" text="Save" />
      <Button fx:id="cancelBtn" layoutX="503.0" layoutY="289.0" mnemonicParsing="false" onAction="#onActionCancelBtn" text="Cancel" />
      <Label fx:id="exceptionsLabel" layoutX="50.0" layoutY="293.0" visible="false" />
   </children>
</AnchorPane>
